import requests


def main():
    url = "{{baseUrl}}/generate/video/runway/generation"
    payload = {
        "model": "{{model}}",
        "character": {
            "type": "image",
            "url": "https://fs.tonkosti.ru/33/ol/33olc2eyzj6s4w8ksg8kkc0gg.jpg",
        },
        "reference": {
            "type": "video",
            "url": "https://www.shutterstock.com/shutterstock/videos/1068048734/preview/stock-footage-cheerful-businessman-talking-on-the-phone-on-a-green-screen-chroma-key.webm",
        }
    }
    headers = {"Authorization": "Bearer {{apiKey}}", "Content-Type": "application/json"}

    response = requests.post(url, json=payload, headers=headers)
    print("Generation:", response.json())

if __name__ == "__main__":
    main()
