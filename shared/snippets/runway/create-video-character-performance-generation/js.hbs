const main = async () => {
  const response = await fetch('{{baseUrl}}/generate/video/runway/generation', {
    method: 'POST',
    headers: {
      Authorization: 'Bearer {{apiKey}}',
      'Content-Type': 'application/json',
    },
    body: JSON.stringify({
      model: '{{model}}',
      character: {
        type: 'image',
        url: 'https://fs.tonkosti.ru/33/ol/33olc2eyzj6s4w8ksg8kkc0gg.jpg',
      },
      reference: {
        type: 'video',
        url: 'https://videos.pexels.com/video-files/3044160/3044160-uhd_2560_1440_24fps.mp4',
      }
    }),
  }).then((res) => res.json());

  console.log('Generation:', response);
};

main()
