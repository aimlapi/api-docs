from openai import OpenAI

client = OpenAI(
    base_url="{{baseUrl}}",
    api_key="{{apiKey}}",    
)

response = client.chat.completions.create(
    model="{{model}}",
    web_search_options={},
    messages=[
        {
            "role": "user",
            "content": "What holiday is today?"
        },
    ],
)

print(response.choices[0])
