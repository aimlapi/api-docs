import requests


def main():
    url = "{{baseUrl}}/video/generations"
    payload = {
        "model": "{{model}}",
        "video_url": "https://storage.googleapis.com/falserverless/model_tests/kling/kling-v2.5-turbo-pro-text-to-video-output.mp4",
        "lip_sync_tts_content": "Hello World",
        "lip_sync_tts_speaker": "Harper",
    }
    headers = {"Authorization": "Bearer {{apiKey}}", "Content-Type": "application/json"}

    response = requests.post(url, json=payload, headers=headers)
    print("Generation:", response.json())


if __name__ == "__main__":
    main()
