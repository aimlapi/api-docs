const main = async () => {
  const response = await fetch('{{baseUrl}}/video/generations', {
    method: 'POST',
    headers: {
      Authorization: 'Bearer {{apiKey}}',
      'Content-Type': 'application/json',
    },
    body: JSON.stringify({
      model: '{{model}}',
      video_url: 'https://storage.googleapis.com/falserverless/model_tests/kling/kling-v2.5-turbo-pro-text-to-video-output.mp4',
      lip_sync_tts_content: 'Hello World!',
      lip_sync_tts_speaker: 'Harper',
    }),
  }).then((res) => res.json());

  console.log('Generation:', response);
};

main()
