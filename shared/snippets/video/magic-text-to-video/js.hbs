const main = async () => {
  const response = await fetch('{{baseUrl}}/video/generations', {
    method: 'POST',
    headers: {
      Authorization: 'Bearer {{apiKey}}',
      'Content-Type': 'application/json',
    },
    body: JSON.stringify({
      model: '{{model}}',
      prompt: 'Hello world!',
    }),
  }).then((res) => res.json());

  console.log('Generation:', response);
};

main()
