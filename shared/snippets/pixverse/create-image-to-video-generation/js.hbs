const main = async () => {
  const response = await fetch('{{baseUrl}}/generate/video/pixverse/generation', {
    method: 'POST',
    headers: {
      Authorization: 'Bearer {{apiKey}}',
      'Content-Type': 'application/json',
    },
    body: JSON.stringify({
      model: '{{model}}',
      prompt: 'Mona Lisa puts on glasses with her hands.',
      image_url: 'https://storage.googleapis.com/falserverless/USO/style3.webp'
    }),
  }).then((res) => res.json());

  console.log('Generation:', response);
};

main()
